{
    "name": "Read Later",
    "description": "Save articles to read later",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "cornsilk",
    "icons": [
        {
            "src": "/static/icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/static/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "share_target": {
        "action": "/share-target/",
        "enctype": "application/x-www-form-urlencoded",
        "method": "GET",
        "params": {
            "title": "title",
            "text": "text"
        }
    }
}
